main() { 
  
  var list = [1,2,3,4,5,6,7,8];
  
  print(list.length);
  
  list.sort();
  list = list.reversed.toList();
  print(list);
  
  
  var newList = list.sublist(0,3);
  print(newList);
  
  print(list.indexOf(5));
  
  list.removeWhere((item)=> item >= 5 && item <= 8);
  print(list);
  
  var arr = [1,2,3];
  
  list.remove(1);
  list.remove(2);
  list.remove(3);
  
  list.addAll([10,20,30]);
  print(list);
}  
